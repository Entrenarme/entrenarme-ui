    "@storybook/addon-viewport": "^4.0.0-alpha.11",
    "@storybook/addons": "^4.0.0-alpha.11",
    "@storybook/react": "^4.0.0-alpha.11",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-plugin-flow-react-proptypes": "^24.0.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-runtime": "^6.26.0",
    "dom-testing-library": "^2.7.0",
    "flow-bin": "^0.76.0",
    "jest-styled-components": "^5.0.1",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-app-rewire-styled-components": "^3.0.2",
    "react-app-rewired": "^1.5.2",
    "react-dom": "^16.4.1",
    "react-scripts": "1.1.4",
    "react-testing-library": "^4.1.2"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build:babel": "rm -rf dist && NODE_ENV=production babel src/lib --plugins transform-es2015-modules-commonjs --out-dir dist --copy-files --ignore __tests__,spec.js,test.js,__snapshots__,stories.js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "build:app": "react-app-rewired build",
    "build": "yarn build:babel && cp ./package.json ./dist",
    "publish:npm": "yarn build && npm publish ./dist",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.0-14",
    "@fortawesome/free-solid-svg-icons": "^5.1.0-11",
